    "@types/sinon": "10.0.11",
    "@types/superagent": "^4.1.15",
    "@types/validator": "^13.7.3",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "chai": "4.3.6",
    "chai-http": "4.3.0",
    "eslint": "^8.18.0",
